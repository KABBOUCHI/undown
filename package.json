{
  "name": "undown",
  "version": "0.0.11",
  "description": "Markdown component for Vue",
  "repository": "KABBOUCHI/undown",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "vitest dev",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "eslint . --fix && prettier -w .",
    "prepack": "pnpm build"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^22.10.2",
    "eslint": "^9.17.0",
    "eslint-config-unjs": "^0.4.2",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "unbuild": "^3.0.1"
  },
  "packageManager": "pnpm@9.15.1",
  "dependencies": {
    "domhandler": "^5.0.3",
    "htmlparser2": "^10.0.0",
    "markdown-it": "^14.1.0"
  },
  "peerDependencies": {
    "vue": "^3.5.13"
  }
}
